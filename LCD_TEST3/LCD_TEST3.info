#include <LiquidCrystal.h>
LiquidCrystal lcd(8, 9, 4, 5, 6, 7);

void setup()
{
  lcd.begin(16, 2);
  lcd.clear();
  lcd.setCursor(0, 0);
  lcd.print("LCD Sample v1.0");
  delay(1000);
  lcd.clear();
}
void loop()
{
  lcd.setCursor(0, 0);
  lcd.print("Hello World!!");
  lcd.setCursor(0, 1);
  lcd.print("Hello World!!!");
}
